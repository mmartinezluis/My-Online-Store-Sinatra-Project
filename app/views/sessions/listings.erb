<h1>All My Listings</h1>
<section class="boxes" id="sessions-listings">
  <% @user.listings.each do |item| %>
    <div class="item-box" id="sessions-item-box">
      <h1><%= item.name %></h1><br>
      <%= item.show_price %><br>
      Stock: <%= item.stock %>
      <a href="/items/<%= item.id %>/edit" class="button" id="edit-listing-link">Edit Listing</a>
      <form action="/items/<%= item.id %>" method="post">
        <input type="hidden" id="hidden" name="_method" value="delete">
        <input type="submit" value="Delete" class="button" id="delete-button">
      </form>
    </div> 
  <% end %>
</section>
