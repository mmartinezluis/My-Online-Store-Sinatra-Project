<h1>All Purchases</h1>
<section class="boxes" id="sessions-purchases">
  <% @user.purchases.each do |item| %>
    <div class="item-box" id="sessions-purchases-item-box">
      <h1><%= item.name %></h1>
      <span><%= item.show_price %></span>
      <span>Quantity: <%= item.quantity_purchased %></span> 
    </div>
  <% end %>
  <span><a href="/users/<%= @user.slug %>">Back to account</a></span>
</section>