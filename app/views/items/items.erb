<h1>Listings</h1>
<section class="boxes" id="items-index">
  <% User.all_users_listings.each do |item| %>
    <div class="item-box" id="items-item">
      <% if item.status == "listing" %>
        <% if item.user == current_user %>
          <%= item.name %><br><%= item.show_price %><br>Seller: <%= item.user.username %><br>Stock: <%= item.stock %>
        <% else %>
          <a href="/items/<%= item.id %>"><%= item.name %><br><%= item.show_price %><br>Seller: <%= item.user.username %><br>Stock: <%= item.stock %></a>
        <% end %>
      <% end %>
    </div>
  <% end %>
</section>